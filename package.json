  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.3",
    "bs-platform": "^4.0.5",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "nearley": "^2.15.1"
  }
}
